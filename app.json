{
  "expo": {
    "name": "GirlMath",
    "slug": "girlmath-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB6D9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.girlmath.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserNotificationUsageDescription": "GirlMath sends reminders before your bills are due so you never miss a payment ðŸ’¸"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.POST_NOTIFICATIONS", "android.permission.RECEIVE_BOOT_COMPLETED"]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FFB6D9",
          "defaultChannel": "bills"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
